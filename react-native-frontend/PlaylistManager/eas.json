{
  "cli": {
    "version": ">= 14.5.0",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "env": {
        "AUTH0_DOMAIN": "${AUTH0_DOMAIN}",
        "AUTH0_CLIENT_ID": "${AUTH0_CLIENT_ID}"
      }
    },
    "preview": {
      "distribution": "internal",
      "env": {
        "AUTH0_DOMAIN": "${AUTH0_DOMAIN}",
        "AUTH0_CLIENT_ID": "${AUTH0_CLIENT_ID}"
      }
    },
    "production": {
      "autoIncrement": true,
      "env": {
        "AUTH0_DOMAIN": "${AUTH0_DOMAIN}",
        "AUTH0_CLIENT_ID": "${AUTH0_CLIENT_ID}"
      }
    }
  },
  "submit": {
    "production": {}
  }
}
